{
   "name": "@zesty-io/explorer",
   "version": "v1.2.12",
   "description": "",
   "main": "./dist/explorer.production.js",
   "scripts": {
      "start": "webpack serve --mode development",
      "build": "webpack --mode production --config=webpack.config.common.ts",
      "build-dev": "webpack --config=webpack.config.dev.ts",
      "prepare": "husky install",
      "check-types": "tsc --pretty --noEmit",
      "check-format": "prettier --check .",
      "check-lint": "eslint . --ext ts --ext tsx --ext js",
      "format": "prettier --write .",
      "test-all": "npm run check-format && npm run check-lint && npm run check-types",
      "commit": "npm run build && git add . && cz",
      "submit": "npm run build && git add . && cz && npm run release",
      "release": "release-it --no-npm",
      "watch": "webpack --watch --config=webpack.config.common.ts"
   },
   "keywords": [
      "zesty-io",
      "headless-cms",
      "typescript",
      "zesty",
      "cms"
   ],
   "author": "zestyionpm",
   "license": "ISC",
   "dependencies": {
      "crypto-browserify": "^3.12.0",
      "dotenv": "^16.0.0",
      "global": "^4.4.0",
      "react": "^18.1.0",
      "react-dom": "^18.1.0"
   },
   "devDependencies": {
      "@babel/core": "^7.17.10",
      "@babel/plugin-transform-runtime": "^7.17.10",
      "@babel/preset-env": "^7.17.10",
      "@babel/preset-react": "^7.16.7",
      "@babel/preset-typescript": "^7.16.7",
      "@babel/runtime": "^7.17.9",
      "@emotion/react": "^11.9.0",
      "@emotion/styled": "^11.8.1",
      "@material-ui/core": "^4.12.4",
      "@mui/icons-material": "^5.8.0",
      "@mui/material": "^5.6.2",
      "@types/dotenv-webpack": "^7.0.3",
      "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
      "@types/react": "^18.0.9",
      "@types/react-dom": "^18.0.3",
      "@types/react-helmet": "^6.1.5",
      "@types/webpack": "^5.28.0",
      "@types/webpack-dev-server": "^4.7.2",
      "@types/webpack-env": "^1.16.4",
      "@typescript-eslint/eslint-plugin": "^5.22.0",
      "@typescript-eslint/parser": "^5.22.0",
      "babel-loader": "^8.2.5",
      "babel-plugin-module-resolver": "^4.1.0",
      "clean-webpack-plugin": "^4.0.0",
      "cz-conventional-changelog": "^3.3.0",
      "dotenv-webpack": "^7.1.0",
      "eslint": "^8.15.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-import-resolver-webpack": "^0.13.2",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-react": "^7.29.4",
      "eslint-plugin-react-hooks": "^4.5.0",
      "fork-ts-checker-webpack-plugin": "^7.2.11",
      "fuse.js": "^6.5.3",
      "husky": "^7.0.4",
      "prettier": "^2.6.2",
      "process": "^0.11.10",
      "react-code-blocks": "^0.0.9-0",
      "react-helmet": "^6.1.0",
      "react-json-view-ssr": "^1.19.1",
      "release-it": "^15.0.0",
      "terser-webpack-plugin": "^5.3.1",
      "ts-node": "^10.7.0",
      "typescript": "^4.6.4",
      "webpack": "^5.72.0",
      "webpack-cli": "^4.9.2",
      "webpack-dev-server": "^4.9.0"
   },
   "browser": {
      "[module-name]": false
   },
   "config": {
      "commitizen": {
