      "cz-conventional-changelog": "^3.3.0",
      "dotenv": "^16.0.1",
      "dotenv-webpack": "^7.1.0",
      "eslint": "^8.18.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-import-resolver-webpack": "^0.13.2",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-react": "^7.29.4",
