      "svg-url-loader": "^7.1.1",
      "terser-webpack-plugin": "^5.3.3",
      "ts-jest": "^28.0.5",
      "ts-node": "^10.9.1",
      "typescript": "^4.7.4",
      "url-loader": "^4.1.1",
      "webpack": "^5.73.0",
      "webpack-bundle-analyzer": "^4.5.0",
      "webpack-cli": "^4.10.0",
      "webpack-dev-server": "^4.9.3"
   },
   "repository": {
      "type": "git",
      "url": "git@github.com:zesty-io/live-editor.git"
   },
   "browser": {
      "[module-name]": false
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog"
      }
   },
   "engines": {
      "node": ">=14.0.0"
   },
   "release-it": {
      "github": {
         "release": true
      }
   }
}
