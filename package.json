      "webpack-bundle-analyzer": "^4.5.0",
      "webpack-cli": "^4.10.0",
      "webpack-dev-server": "^4.9.2"
   },
   "browser": {
      "[module-name]": false
   },
   "config": {
      "commitizen": {
         "path": "./node_modules/cz-conventional-changelog"
      }
   },
   "release-it": {
      "github": {
         "release": true
      }
   }
